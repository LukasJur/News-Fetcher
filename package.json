{
  "name": "news-fetcher",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LukasJur/News-Fetcher.git"
  },
  "description": "Website utilizing News API",
  "main": "index.js",
  "browserslist": [
    ">0.25%",
    "IE 10"
  ],
  "scripts": {
    "build": "webpack",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "node_start": "node ./dist/index.js",
    "start": "nodemon ./dist/index.js",
    "sdev": "webpack-dev-server",
    "test": "jest ./src",
    "clean": "rimraf dist node_modules",
    "lint": "eslint src -c .eslintrc --ext js,jsx",
    "lint-fix": "eslint src -c .eslintrc --ext js,jsx --fix",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax=scss",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/enzyme.setup.js",
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "author": "Lukas Jurevicius",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.9.1",
    "acorn": "^6.0.5",
    "autoprefixer": "^9.4.6",
    "babel-core": "^7.0.0-bridge.0",
    "connected-react-router": "^6.2.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.5.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
    "history": "^4.7.2",
    "newsapi": "^2.4.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "raf": "^3.4.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-with-styles": "^3.2.1",
    "redux": "^4.0.1",
    "rimraf": "^2.6.3",
    "webpack": "^4.28.4",
    "webpack-hot-client": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.12.1",
    "eslint-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.7.0",
    "style-loader": "^0.23.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-node-externals": "^1.7.2"
  }
}
